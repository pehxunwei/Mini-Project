@model Mini_Project.Models.Account

@{
    ViewBag.Title = "PasswordChanged";
}

<div class="custom-checkphonenumber" style="margin-bottom: 445px;">
    <div class="form-horizontal">

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        <div h4 style="text-align: justify; font-size: 16px; color: #8e9297; margin-bottom: 30px;">Your password has been reset successfully!</div>


        <div>
            @Html.ActionLink("Back to Settings", "Manage", "Account")
        </div>

      
    </div>
</div>


